{
    "cSpell.words": [
        "turbopack",
        "vercel"
    ]
    "[sql]": {
      "editor.formatOnSave": false,
      "editor.defaultFormatter": null
    },
    "files.associations": {
      "*.sql": "sql"
    },
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "files.eol": "\n",
    "files.encoding": "utf8",
    "[prisma]": {
      "editor.defaultFormatter": "Prisma.prisma",
      "editor.formatOnSave": true
    }
}
